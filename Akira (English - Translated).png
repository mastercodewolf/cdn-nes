    var $ = jQuery;

    if (typeof controllers == 'undefined') {
        controllers = {};
    }
    
    cookieconsent.initialise({
        type: 'info',
        palette: {
        "popup":{"background":"#000"},
        "button":{"background":"#ff5b5b","text":"#ffffff"},
        }
