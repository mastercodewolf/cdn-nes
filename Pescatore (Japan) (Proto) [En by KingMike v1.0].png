                            $('.top-button').html(ret.menu);
                            loginModal.foundation('close');
                            if (typeof Game != 'undefined') Game.isLoggedIn = true;
                            $(window).trigger('user.loginSuccess');
                        } else {
                            $.toast('Failed');
                        }
                    }, 'json');
                }
            }, {scope: 'public_profile,email', 'auth_type':'rerequest'});
        });

